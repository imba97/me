    "@vueuse/nuxt": "^11.3.0",
    "defu": "^6.1.4",
    "floating-vue": "^5.2.2",
    "nuxt": "^3.14.1592",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@iconify/json": "^2.2.275",
